let sum = (f: (int) -> int) -> int {
  let n, sum = 1, 0;
  for n <= 10 {
    sum = sum + f(n);
    n = n + 1;
  }
  return sum;
};

let neg = (n: int) -> int {
  return -n;
};

let sqr = (n: int) -> int {
  return n * n;
};

printf("%d, ", sum(neg));
printf("%d, ", sum(sqr));
printf("%d\n", sum((n: int) -> int { return n * 2; }));
